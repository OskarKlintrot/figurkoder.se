{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif,ico}", "/*.{css,scss,js}"]
  },
  "trailingSlash": "never",
  "routes": [
    {
      "route": "/.auth/*",
      "statusCode": 404
    }
  ],
  "mimeTypes": {
    ".webmanifest": "application/manifest+json"
  },
  "globalHeaders": {
    "Permission-Policy": "unload()"
  }
}
